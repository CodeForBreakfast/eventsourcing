{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "ignore": ["packages/*/build.{js,ts,mjs}", "**/*.example.ts"],
  "ignoreDependencies": [
    "@commitlint/cli",
    "@effect/language-service",
    "@effect/platform",
    "@effect/platform-bun",
    "eslint-plugin-*",
    "fast-check",
    "npm",
    "@codeforbreakfast/eslint-effect",
    "@codeforbreakfast/buntest",
    "@codeforbreakfast/eventsourcing-testing-contracts",
    "type-fest",
    "@codeforbreakfast/eventsourcing-store",
    "@effect/experimental",
    "@effect/sql-pg",
    "@codeforbreakfast/eventsourcing-transport",
    "@codeforbreakfast/eventsourcing-transport-websocket",
    "@codeforbreakfast/eventsourcing-protocol",
    "effect"
  ],
  "ignoreBinaries": ["publish"],
  "workspaces": {
    "packages/eslint-effect": {
      "entry": ["src/index.js", "test/**/*.ts"],
      "project": ["src/**/*.js", "test/**/*.ts"]
    },
    "packages/*": {
      "entry": ["src/index.{ts,js,mjs}"],
      "project": ["src/**/*.{ts,js,mjs}!"]
    }
  }
}
